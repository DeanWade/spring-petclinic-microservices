<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	
	<dubbo:application name="${dubbo.application.name}" />
	
	<dubbo:consumer check="false" />

    <dubbo:registry id="zookeeper" protocol="zookeeper" address="${dubbo.registry.address}" file="${dubbo.registry.file}"/>
    
    <dubbo:protocol name="${dubbo.protocol.name}" port="${dubbo.protocol.port}" />
	
	<dubbo:service interface="org.springframework.samples.petclinic.customers.service.PetService" ref="petService" />
	
	<dubbo:service interface="org.springframework.samples.petclinic.customers.service.OwnerService" ref="ownerService" />
	
</beans>